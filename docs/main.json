    "paths": {
        "/login": {
            "post": {
                "tags": [
                    "Authentication"
                ],
                "description": "Endpoint to use to authenticate with the api",
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "type": "object",
                                "properties": {
                                    "username": {
                                        "type": "string",
                                        "example": "doejohn00"
                                    },
                                    "password": {
                                        "type": "string",
                                        "example": "MySecretPassword"
                                    }
                                }
                            },
                            "examples": {}
                        }
                    },
                    "description": ""
                },
                "responses": {
                    "200": {
                        "description": "Successfully logged in",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "status": {
                                            "type": "boolean",
                                            "default": "true"
                                        },
                                        "user": {
                                            "type": "string",
                                            "example": "doejohn00"
                                        },
                                        "firstlogin": {
                                            "type": "boolean",
                                            "example": "true"
                                        },
                                        "token": {
                                            "$ref": "#/components/schemas/token"
                                        }
                                    }
                                }
                            }
                        }
                    },
                    "401": {
                        "description": "Login Failed",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "status": {
                                            "type": "boolean",
                                            "default": "false"
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },
        "/logout": {
            "get": {
                "tags": [
                    "Authentication"
                ],
                "description": "Log out",
                "parameters": [
                    {
                        "name": "username",
                        "description": "The username of the account that is to be logged out",
                        "schema": {
                            "type": "string",
                            "example": "doejohn00"
                        },
                        "in": "query"
                    },
                    {
                        "name": "token",
                        "description": "The token of the account that is to be logged out",
                        "schema": {
                            "$ref": "#/components/schemas/token"
                        },
                        "in": "query"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Succesfully logged out",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "status": {
                                            "type": "string",
                                            "default": "Logged Out"
                                        }
                                    }
                                }
                            }
                        }
                    },
                    "401": {
                        "description": "Unauthorized",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "status": {
                                            "type": "string",
                                            "default": "Unauthorized"
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        }
    }
